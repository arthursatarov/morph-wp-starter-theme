{
  "name": "arthursatarov/morph-wp-starter-theme",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer/installers": "^2.0",
    "wpackagist-plugin/cyr2lat": "^6.1",
    "wpengine/advanced-custom-fields-pro": "^6.3"
  },
  "require-dev": {
    "wpackagist-plugin/query-monitor": "^3.15"
  },
  "extra": {
    "installer-paths": {
      "www/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "www/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
